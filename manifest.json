{
    "name": "Crawler",
    "version": "4.0",
    "manifest_version": 2,
    "description": "Reunion Crawler",
    "browser_action": {
        "default_popup" :"popup.html" ,
        "default_icon": "reunion-r.png"
    },

    "homepage_url": "http://localhost:3001",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "notifications"
    ]  ,
    "background": {
        "scripts": ["background.js"]
    }
}
